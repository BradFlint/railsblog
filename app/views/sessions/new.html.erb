<h1>Sessions#new</h1>

<div align=center id="logingreet">
	<h1>Welcome Back! Sign in here</h1>
</div>
<!-- form for returning user to log in -->
<div align= center id="loginform">
	<%= form_tag("/login", method: "post") do %>
		<%= label_tag(:username, "User Name")%>
		<%= text_field_tag(:username)%>
		<%= label_tag(:password, "Password")%>
		<%= password_field_tag(:password) %>
		<%= submit_tag("submit") %>
	<% end %>
</div>
